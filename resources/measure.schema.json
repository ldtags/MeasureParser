{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "eTRM Measure",
    "description": "A file that describes an eTRM measure",
    "type": "object",
    "properties": {
        "owned_by_user": {
            "type": "string"
        },
        "determinants": {
            "description": "Measure specific (non-shared) parameter",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": { "$ref": "#/$defs/name" },
                    "api_name": { "$ref": "#/$defs/api_name" },
                    "labels": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "name": { "$ref": "#/$defs/name" },
                                "api_name": { "$ref": "#/$defs/api_name" },
                                "active": {
                                    "type": "boolean"
                                },
                                "description": { "$ref": "#/$defs/description" }
                            }
                        }
                    },
                    "description": { "$ref": "#/$defs/description" },
                    "order": { "$ref": "#/$defs/order" },
                    "reference_refs": { "$ref": "#/$defs/reference_refs" }
                }
            }
        },
        "shared_determinant_refs": {
            "description": "Shared parameter",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "order": { "$ref": "#/$defs/order" },
                    "version": { "$ref": "#/$defs/version" },
                    "active_labels": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": ["order", "version", "active_labels"]
            }
        },
        "shared_lookup_refs": {
            "description": "Shared value table",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "order": { "$ref": "#/$defs/order" },
                    "version": { "$ref": "#/$defs/version" }
                },
                "required": ["order", "version"]
            }
        },
        "value_tables": {
            "description": "Measure specific (non-shared) value table",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": { "$ref": "#/$defs/name" },
                    "api_name": { "$ref": "#/$defs/api_name" },
                    "type": {
                        "type": "string"
                    },
                    "description": { "$ref": "#/$defs/description" },
                    "order": { "$ref": "#/$defs/order" },
                    "determinants": { "$ref": "#/$defs/determinants" },
                    "columns": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "name": { "$ref": "#/$defs/name" },
                                "api_name": { "$ref": "#/$defs/api_name" },
                                "unit": { "$ref": "#/$defs/unit" },
                                "reference_refs": { "$ref": "#/$defs/reference_refs" }
                            }
                        }
                    },
                    "values": {
                        "type": "array",
                        "items": {
                            "type": "array",
                            "items": {
                                "type": ["string", "null"]
                            }
                        }
                    },
                    "reference_refs": { "$ref": "#/$defs/reference_refs" }
                }
            }
        },
        "calculations": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": { "$ref": "#/$defs/name" },
                    "api_name": { "$ref": "#/$defs/api_name" },
                    "order": { "$ref": "#/$defs/order" },
                    "unit": { "$ref": "#/$defs/unit" },
                    "determinants": { "$ref": "#/$defs/determinants" },
                    "values": {
                        "type": "array",
                        "items": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "reference_refs": { "$ref": "#/$defs/reference_refs" }
                }
            }
        },
        "exclusion_tables": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": { "$ref": "#/$defs/name" },
                    "api_name": { "$ref": "#/$defs/api_name" },
                    "order": { "$ref": "#/$defs/order" },
                    "determinants": { "$ref": "#/$defs/determinants" },
                    "values": {
                        "type": "array",
                        "items": {
                            "type": "array",
                            "items": {
                                "type": ["string", "boolean"]
                            }
                        }
                    }
                }
            }
        },
        "ds_additional_fields": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "users": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "role": {
                        "type": "integer"
                    }
                }
            }
        },
        "MeasureID": {
            "type": "string"
        },
        "MeasureVersionID": {
            "type": "string"
        },
        "MeasureName": {
            "type": "string"
        },
        "UseCategory": {
            "type": "string"
        },
        "PALead": {
            "type": "string"
        },
        "StartDate": {
            "type": "string"
        },
        "EndDate": {
            "type": ["string", "null"]
        },
        "Status": {
            "type": "string"
        }
    },
    "required": ["owned_by_user", "determinants", "shared_determinant_refs",
                 "shared_lookup_refs", "value_tables", "calculations",
                 "exclusion_tables"],
    "$defs": {
        "name": { "type": "string" },
        "api_name": { "type": "string" },
        "order": { "type": "integer" },
        "unit": { "type": "string" },
        "description": { "type": ["string", "null"] },
        "determinants": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "reference_refs": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "version": {
            "type": "object",
            "properties": {
                "version_string": {
                    "type": "string"
                }
            }
        }
    }
}